{
  "name": "venomous-ui",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "storybook dev --port 58080",
    "build:storybook": "storybook build",
    "build:types": "tsc -p tsconfig.types.json",
    "build:libs": "vite build",
    "check:eslint": "eslint . --ext ts,tsx --cache --cache-location=./.cache/.eslintcache",
    "check:prettier": "prettier --check .",
    "format:eslint": "eslint --fix .",
    "format:prettier": "prettier --write . --cache --cache-location=./.cache/.prettiercache",
    "check:all": "concurrently --names \"eslint,prettier\" \"yarn check:eslint\" \"yarn check:prettier\"",
    "format:all": "concurrently --names \"eslint,prettier\" \"yarn format:eslint\" \"yarn format:prettier\"",
    "clean-cache": "rm -rf .cache",
    "prepare": "husky"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^7.0.1",
    "@mui/x-charts": "^7.28.0",
    "@mui/x-data-grid": "^7.28.3",
    "@mui/x-date-pickers": "^7.28.3",
    "dayjs": "^1.11.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@iconify/react": "^5.2.1",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-interactions": "^8.6.12",
    "@storybook/blocks": "^8.6.12",
    "@storybook/manager-api": "^8.6.12",
    "@storybook/react-vite": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@storybook/theming": "^8.6.12",
    "@types/node": "^22.14.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.21.0",
    "eslint-plugin-react-dom": "^1.40.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-react-x": "^1.40.3",
    "eslint-plugin-storybook": "^0.12.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "prettier-plugin-css-order": "^2.1.2",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "prettier-plugin-organize-imports": "^4.1.0",
    "rollup-plugin-visualizer": "^5.14.0",
    "sass": "^1.86.3",
    "storybook": "^8.6.12",
    "stylelint": "^16.17.0",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  },
  "homepage": "https://github.com/BlaxBerry333/venomous-ui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BlaxBerry333/venomous-ui"
  },
  "bugs": {
    "url": "https://github.com/BlaxBerry333/venomous-ui/issues"
  },
  "files": [
    "dist/*.js",
    "dist/*.cjs",
    "dist/types"
  ],
  "sideEffects": [
    "**/*.css",
    "**/*.scss",
    "**/*.sass"
  ],
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs",
      "default": "./dist/index.js"
    }
  }
}
